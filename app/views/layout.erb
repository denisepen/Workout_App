<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Workout Diary</title>
    <link rel="stylesheet"   href="<%= url("/css/master.css")%>"/>
  </head>
  <body>
    <div class="">

      <% if flash[:s_signerror] %>
      <div class="s_signerror"><%= flash[:s_signerror] %></div>
      <% end %>

      <% if flash[:not_user] %>
      <div class="not_user"><%= flash[:not_user] %></div>
      <% end %>

      <% if flash[:signup] %>
      <div class="signup"><%= flash[:signup] %></div>
      <% end %>

      <% if flash[:s_error] %>
      <div class="s_error"><%= flash[:s_error] %></div>
      <% end %>

      <% if flash[:l_error] %>
      <div class="l_error"><%= flash[:l_error] %></div>
      <% end %>

      <% if flash[:notice] %>
      <div class="notice"><%= flash[:notice] %></div>
      <% end %>
      <% if flash[:updated] %>
      <div class="updated"><%= flash[:updated] %></div>
      <% end %>

      <%= yield %>
    </div>

  </body>
</html>
